<openerp>
  <data>
    <!-- Delivery Carriers -->
    <record id="view_stock_picking_form" model="ir.ui.view">
      <field name="name">stock.picking.form.unifaun</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form" />
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="carrier_tracking_ref" position="after">
            <field name="is_unifaun" invisible="1"/>
            <button name="order_transport" string="Order Transport" type='object' attrs="{'invisible': [('is_unifaun', '==', False)]}" />

        </field>
      </field>
    </record>
  </data>
</openerp>